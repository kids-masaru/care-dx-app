# Gemini API設定
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-2.5-pro

# Google Sheets設定
SERVICE_ACCOUNT_PATH=./service_account.json

# デフォルトのスプレッドシートID
ASSESSMENT_SHEET_ID=your_assessment_sheet_id
CASE_MEETING_SHEET_ID=your_case_meeting_sheet_id
MANAGEMENT_MEETING_SHEET_ID=your_management_meeting_sheet_id

# デフォルトのシート名（空白の場合は最初のシートを使用）
DEFAULT_SHEET_NAME=

# テンプレート保護をデフォルトで有効化（true/false）
ENABLE_TEMPLATE_PROTECTION=false
